{
    "name": "andrey-helldar/bad-browser-detect",
    "description": "Determine the minimum acceptable version of the browser and notify the user if the version does not match the required version.",
    "type": "library",
    "license": "MIT",
    "keywords": ["bad browser"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": ">=5.6.9",
        "illuminate/http": "^5.4",
        "illuminate/bus": "^5.4",
        "illuminate/queue": "^5.4",
        "illuminate/contracts": "^5.4",
        "illuminate/mail": "^5.4",
        "jaybizzle/crawler-detect": "^1.2",
        "jenssegers/agent": "^2.6",
        "illuminate/database": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\BadBrowser\\": "src"
        },
        "files": [
            "src/helpers/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\BadBrowser\\ServiceProvider"
            ]
        }
    }
}
